<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">

    <List headerText="{i18n>invoiceTitle}" width="100%" items="{invoices>/Invoices}">
        <items>
            <ObjectListItem title="{invoices>ProductName}">
                <ObjectAttribute text="Quantity {invoices>Quantity}" />
                <ObjectAttribute text="{invoices>ShipperName}" />
            </ObjectListItem>
        </items>
    </List>

</mvc:View>